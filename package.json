  "bugs": {
    "url": "https://github.com/quoc2721/cover-trello-api/issues"
  },
  "homepage": "https://github.com/quoc2721/cover-trello-api#readme",
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "dotenv": "^16.0.1",
    "eslint": "^8.20.0",
    "express": "^4.18.1",
    "joi": "^17.6.0",
    "mongodb": "^4.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/node": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "nodemon": "^2.0.19"
  }
